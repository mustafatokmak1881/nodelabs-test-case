								"value": "Bearer {{auth_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/user/list",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"user",
								"list"
							]
						},
						"description": "Get list of all users. Requires admin role."
					},
					"response": []
				}
			],
			"description": "User management endpoints (admin only)."
		},
		{
			"name": "System",
			"item": [
				{
					"name": "Health Check",
					"request": {
						"method": "GET",
						"header": [],
